#!/bin/bash -x
ARCH=arm \
CC=arm-linux-gnueabi-gcc \
CXX=arm-linux-gnueabi-gcc \
LIBNBIS=no \
./configure \
--target=arm-linux-gnueabi \
--with-gcc=arm-linux-gnueabi-gcc \
--host=arm-linux-gnueabi \
--prefix="$NCURSES_OUT_PATH" \
--enable-static=yes \
--disable-stripping
LIBNBIS=no make
make install
